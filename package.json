{
  "name": "babel-plugin-transform-react-stateless-component-name",
  "version": "1.0.0",
  "description": "A babel plugin to add name for default exported stateless components.",
  "license": "MIT",
  "repository": "wyze/babel-plugin-transform-react-stateless-component-name",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf .nyc_output coverage lib",
    "codecov": "nyc report -r text-lcov | codecov",
    "lint": "eslint src test",
    "prebuild": "npm run clean && npm run test",
    "pretest": "npm run lint",
    "preversion": "npm run build",
    "test": "nyc npm run test:only",
    "test:only": "ava",
    "test:watch": "npm run test:only -- --watch"
  },
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "keywords": [
    "babel-plugin",
    "transform",
    "stateless",
    "functional",
    "component",
    "displayname"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "wyze/base"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "lib/**",
      "node_modules/**"
    ],
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "codecov.io": "^0.1.6",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-wyze": "^2.0.0",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-wyze": "^1.4.0",
    "nyc": "^7.0.0",
    "rimraf": "^2.5.3"
  }
}
