{
  "name": "babel-plugin-transform-react-stateless-component-name",
  "version": "1.0.1",
  "description": "A babel plugin to add name for default exported stateless components.",
  "license": "MIT",
  "repository": "wyze/babel-plugin-transform-react-stateless-component-name",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf .nyc_output coverage lib",
    "codecov": "NODE_ENV=test nyc report -r text-lcov | codecov",
    "lint": "eslint src test",
    "prebuild": "npm run clean && npm test",
    "pretest": "npm run lint",
    "preversion": "npm run build",
    "test": "NODE_ENV=test nyc npm run test:only",
    "test:only": "ava",
    "test:watch": "npm run test:only -- --watch"
  },
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "keywords": [
    "babel-plugin",
    "transform",
    "stateless",
    "functional",
    "component",
    "displayname"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "wyze/base"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "instrument": false,
    "require": [
      "babel-register"
    ],
    "sourceMap": false
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "codecov.io": "^0.1.6",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-wyze": "^4.0.0-beta.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-wyze": "^2.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4"
  }
}
